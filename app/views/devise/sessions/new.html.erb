<div class="input-container">
  <h2>Log in</h2>

  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-inputs">
      <%= f.input :email,
                  required: false,
                  autofocus: true,
                  input_html: { autocomplete: "email" } %>
      <%= f.input :password,
                  required: false,
                  input_html: { autocomplete: "current-password" } %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, "Log in", class: "btn btn-action" %>
    </div>
  <% end %>
  <br>
  <%= render "devise/shared/links"%>
  <br>
  <div class="default">
  <p class="details-info">If you do not want to create an account. You can use the below default account to login</p>
  <p>Email: jane@test.com</p>
  <p>Password: 123456</p>
  </div>
</div>
